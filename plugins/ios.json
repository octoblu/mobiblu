            "NSLocationUsageDescription": {
                "<string>Is required -for iOS 6 and above- to run the app in background!</string>": 1
            }
        }
    },
    "installed_plugins": {
        "de.appplant.cordova.plugin.background-mode": {
            "PACKAGE_NAME": "com.appgyver.helloSteroids"
        }
    },
    "dependent_plugins": {}
}